<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width" />
  <link href="~/Content/bootstrap.css" rel="stylesheet" />
  <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
  <title>@ViewBag.Title</title>
</head>
<body>
  <nav>
    <ul class="nav nav-pills">
      <li>@Html.ActionLink("Home", "Index", "Home")</li>
           <li>@Html.ActionLink("About", "About", "Home")</li>
           <li>@Html.ActionLink("Persons", "Index", "Person")</li>
   
      <li class="dropdown">
        <a class="dropdown-toggle" data-toggle="dropdown" href="#">Admin <span class="caret"></span>
        </a>
        <ul class="dropdown-menu" role="menu">
          <li>@Html.ActionLink("Persons", "Index", "Person")</li>
   
        </ul>
      </li>
      <li class="dropdown">
        <a class="dropdown-toggle" data-toggle="dropdown" href="#">TestPages <span class="caret"></span>
        </a>
        <ul class="dropdown-menu" role="menu">
          <li>@Html.ActionLink("TestPage", "TestPage", "Home")</li>
          <li>@Html.ActionLink("Angular", "Angular", "Home")</li>
          <li>@Html.ActionLink("AngularSmall", "AngularSmall", "Home")</li>

        </ul>
      </li>


    </ul>
  </nav>
  <div class="container well">
    @RenderBody()
  </div>

  <footer>
    <div class="well">Copyright @DateTime.Now.Year TicItNow</div>

  </footer>
  <script src="~/Scripts/jquery-2.1.1.js"></script>
  <script src="~/Scripts/jquery-ui-1.10.4.js"></script>
  <script src="~/Scripts/bootstrap.js"></script>
  <script src="~/Scripts/angular.js"></script>
  <script>
    $(".dp").datepicker();
  </script>
  @RenderSection(name: "scripts", required: false)
  
</body>
</html>
